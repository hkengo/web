<h1>すべての投稿一覧</h1>h1>

<table>
  <tr>
    <td>タイトル</td>
    <td>内容</td>
    <td></td>
    <td></td>
    <td></td>
  </tr>
  <%- @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td><%= post.content %></td>
    <td><%= link_to "詳細", post_path(post) %></td>
    <td><%= link_to "編集", edit_post_path(post) %></td>
    <td><%= link_to "削除", post_path(post), method: :delete, data: { confirm: '削除しますか？' } %></td>
  </tr>
  <%- end %>
</table>

<%= link_to "新規投稿", new_post_path %>